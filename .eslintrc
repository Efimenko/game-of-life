{
  "plugins": [
    "react",
    "react-hooks",
    "prettier"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "globals": {
    "document": true,
    "console": true,
    "window": true,
    "setInterval": true,
    "clearInterval": true,
    "module": true,
    "test": true,
    "expect": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "no-var": "error",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "error",
    "react/prop-types": 2,
    "prettier/prettier": "error",
    "react/no-multi-comp": 1
  }
}
